{
  "name": "demchenkooleh/sharefile-api",
  "description": "A minimal implementation of ShareFile Api",
  "keywords": [
    "sharefile-api",
    "sharefile",
    "api",
    "php"
  ],
  "homepage": "https://github.com/demchenkooleh/sharefile-api",
  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "guzzlehttp/guzzle": "^6.3",
    "ext-json": "*"
  },
  "require-dev": {
    "larapack/dd": "^1.1",
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Citrix\\Sharefile\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Citrix\\Sharefile\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
